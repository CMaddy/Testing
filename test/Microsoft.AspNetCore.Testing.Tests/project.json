  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-rc2-3002702",
          "type": "platform"
        },
        "System.Diagnostics.Process": "4.1.0-rc2-24027"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
