{
  "description": "Various helpers for writing tests that use ASP.NET Core.",
  "version": "1.1.0-rtm-22752",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/testing"
    },
    "tags": [
      "aspnetcore"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.RuntimeEnvironment.Sources": {
      "version": "1.1.0-rtm-22752",
      "type": "build"
    },
    "NETStandard.Library": "1.6.1",
    "Newtonsoft.Json": "9.0.1",
    "xunit": "2.2.0-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.Reflection": {
          "type": "build"
        },
        "System.Threading.Tasks": {
          "type": "build"
        },
        "System.Xml": {
          "type": "build"
        },
        "System.Xml.Linq": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Diagnostics.Process": "4.3.0",
        "System.Threading.Thread": "4.3.0"
      }
    }
  }
}